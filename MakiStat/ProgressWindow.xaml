<Window x:Class="MakiStat.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MakiStat"
        mc:Ignorable="d"
        Title="Generating website..." Height="141" Width="601">
	<Grid Margin="20">
		<ProgressBar x:Name="genStatus" Minimum="0" Maximum="100" VerticalAlignment="Bottom" Height="50" />
		<Label x:Name="pagePath" Content="Website is generating..." HorizontalAlignment="Center" VerticalAlignment="Top" MinWidth="551" />
	</Grid>
</Window>
